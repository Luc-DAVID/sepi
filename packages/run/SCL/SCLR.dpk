package SCLR;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Sepi Code Library'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  rtl,
  vcl,
  vclshlctrls,
  vclx,
  VclSmp;

contains
  ScDateTimeUtils in 'ScDateTimeUtils.pas',
  ScExtra in 'ScExtra.pas',
  ScLists in 'ScLists.pas',
  ScLOGFile in 'ScLOGFile.pas',
  ScMaths in 'ScMaths.pas',
  ScMD5 in 'ScMD5.pas',
  ScMusicUtils in 'ScMusicUtils.pas',
  ScNoSecretWindow in 'ScNoSecretWindow.pas',
  ScConsts in 'ScConsts.pas',
  ScStrUtils in 'ScStrUtils.pas',
  ScUtils in 'ScUtils.pas';

end.

